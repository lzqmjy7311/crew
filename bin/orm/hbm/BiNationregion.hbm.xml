<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.mng">
	<class
		name="BiNationregion"
		table="BI_NATIONREGION"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="NATIONREGION_CODE"
		>
		</id>

		<property
			name="chinaName"
			column="CHINA_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="nationregionNumber"
			column="NATIONREGION_NUMBER"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="chinaShortName"
			column="CHINA_SHORT_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="engName"
			column="ENG_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="engShortName"
			column="ENG_SHORT_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="st"
			column="ST"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lock"
			column="IS_LOCK"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="del"
			column="IS_DEL"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="crtDt"
			column="CRT_DT"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="lastUpdOper"
			column="LAST_UPD_OPER"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdTms"
			column="LAST_UPD_TMS"
			type="string"
			not-null="false"
			length="14"
		/>

	</class>
</hibernate-mapping>