<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	  navigate:导航信息
	  type:方式（sql|call)
	  interface:是否显示输入条件页面(true|false)
	  databusid:数据总线编号
	  transdataactionurl:数据整理URL
	  async:异步提交标志
	  pagesize:页面显示记录数(缺省为10)
-->
<CommQry
title="票据下拉菜单"
navigate="票据下拉菜单"
type="call"
interface="true"
pagesize="10"
async="true"
transdataactionurl="/trans/TransDataAction.do"
getterclassname="com.huateng.ebank.business.common.getter.DraftsSelectGetter"
>
	<Include id="BankParam"/>
	<Fields>
	<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
	<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->

		<Field id="stdbillnum" desc="电子票号" status="F" method="None" readOnly="true" xpath="/stdbillnum"
			primary="true" type="String" size="20" default="" tip="电子票号" width="170"/>

		<Field id="stdpmmoney" desc="票面金额" status="F" method="None" readOnly="true" xpath="/stdpmmoney"
			primary="true" type="String" size="20" default="" tip="票面金额" width="170"/>
	</Fields>
	<Parameters>
	</Parameters>

	<Where>
	</Where>
	<Operations>
	</Operations>
</CommQry>