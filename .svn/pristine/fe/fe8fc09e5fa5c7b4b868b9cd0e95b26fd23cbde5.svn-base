	<ResultViewParam>/fpages/parammng/ftl/WorkFlowParam.ftl</ResultViewParam>
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
