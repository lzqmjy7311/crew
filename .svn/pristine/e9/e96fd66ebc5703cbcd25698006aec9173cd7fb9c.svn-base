			   edittype="select" dropdowntype="dynamic" init="true" size="10" />
        
        <Field id="opinion" desc="意见" edittype="textarea" xpath="/opinion" height="100" width="700" />
		<Field id="measCompleteDate" status="N" desc="措施完成时间" edittype="postdate"
