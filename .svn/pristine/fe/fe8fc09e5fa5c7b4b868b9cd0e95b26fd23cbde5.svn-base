			method="None" primary="false" type="String" size="100" default=""
			xpath="/taskNameName" tip="任务名" />

 		<Field id="apptype" desc="申请类型" status="F" width="170"
			edittype="select" translated="CQ:WfpappTypeSelect" require=""
