		</div>
</td></tr>
<tr><td>
<@CommonQueryMacro.CommonQuery id="TCwCreditAssetsMonitor" init="true" submitMode="current">
			<@CommonQueryMacro.DataTable id="datatable1" paginationbar="" maxRow="3" fieldStr="INDEX_CODE[70],INDEX_NAME[200],COLO[70],MONITOR_FRE[70],NOW_VAL[70],TOP_VAL[70],YC_VAL[70],BSQ[70],BYC[70],THRESHOLD1[70],THRESHOLD2[70],opr[50]"  width="100%" hasFrame="true"/>
</@CommonQueryMacro.CommonQuery>
</td></tr>
<tr><td>
		<div class="group" noextra="true" style="margin-top: 20px;">
			<h5 noextra="true">资产迁徙-集中度</h5>
		</div>
</td></tr>
<tr><td>
<@CommonQueryMacro.CommonQuery id="TCwCreditAssetsMonitor2" init="true" submitMode="current">
