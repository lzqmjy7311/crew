		<Field id="tlrno" desc="操作员号" status="N" method="None"
			readonly="true" primary="false" type="String" size="20"
			xpath="/tlrno" />
			
		<Field id="isDefault" xpath="/isDefault" desc="是否默认" edittype="select"  translated="LIST:0,否;1,是" datatype="string" />
	</Fields>

	<Where>

	</Where>
	<Operations>
