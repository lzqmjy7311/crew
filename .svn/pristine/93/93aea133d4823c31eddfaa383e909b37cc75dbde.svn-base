	private TlrInfo tlrInfo;
	private RepList<TlrRoleRel> roleRellist;
	private RepList<TlrBctlRel> bctlRellist;
	private RepList<UserOrgRoleRel> rellist;

	public TlrInfo getTlrInfo() {
