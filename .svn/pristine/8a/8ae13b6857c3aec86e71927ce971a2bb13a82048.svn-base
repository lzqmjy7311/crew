<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.mng">
	<class
		name="TblCronTaskJob"
		table="TBL_CRON_TASK_JOB"
	>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid" />  
		</id>

		<property
			name="jobno"
			column="JOBNO"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="processFunction"
			column="PROCESS_FUNCTION"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="processParam"
			column="PROCESS_PARAM"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="maxproc"
			column="MAXPROC"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="runtime"
			column="RUNTIME"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="daysOfMonth"
			column="DAYS_OF_MONTH"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="repeatTime"
			column="REPEAT_TIME"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="repeatCnt"
			column="REPEAT_CNT"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="startTime"
			column="START_TIME"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="endTime"
			column="END_TIME"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="lastRunTime"
			column="LAST_RUN_TIME"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="dueTime"
			column="DUE_TIME"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="sucFlag"
			column="SUC_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="failFlag"
			column="FAIL_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="auto"
			column="AUTO"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lockOwn"
			column="LOCK_OWN"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="desc0"
			column="DESC0"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="desc1"
			column="DESC1"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="desc2"
			column="DESC2"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="string"
			not-null="false"
			length="14"
		/>
		<!--BMSA-40 4Eye功能开发—周涛 定时任务管理-->
		<property
			name="dualcontrolLockstatus"
			column="Dualcontrol_Lockstatus"
			type="string"
			not-null="false"
			length="1"
		/>
		<!--BMSA-40 4Eye功能开发—周涛 定时任务管理-->
	</class>
</hibernate-mapping>