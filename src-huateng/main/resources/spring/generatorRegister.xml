<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "/WEB-INF/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="GetSeqnoGenerator"
		class="com.huateng.ebank.business.common.generator.GetSeqnoGenerator">
		<property name="id">
			<value>GetSeqnoGenerator</value>
		</property>
		<property name="desc">
			<value>序号生成器</value>
