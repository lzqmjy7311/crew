<?xml version="1.0" encoding="UTF-8"?>

<CommQry title="DataDicEntry.title" navigate="数据字典" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="com.huateng.ebank.business.pages.getter.FPPageItemGetter">
	<Include id="BankParam" />
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
        <Field id="select"  xpath="/select" />
		<Field id="id"  desc="组合对象编号" edittype="text" datatype="string" size="32" xpath="/id" />
		<Field id="name" desc="组合对象ID" edittype="text" size="128" xpath="/name" width="300" require="true"/>
		<Field id="descInfo" desc="描述信息" edittype="textarea" status="F" datatype="string" size="256" xpath="/descInfo" width="300" require="true"/>
		<Field id="typeid" desc="关联序号" edittype="text"  datatype="string" size="32" xpath="/typeid" width="300" require="true"/>
		<Field id="viewtype"  desc="组装类型" edittype="select" translated="LIST:0,tab;1,accordion;2,tree" xpath="/viewtype" width="300" require="true"/>
		<Field id="defaultno" desc="默认打开编号" edittype="text" datatype="string" size="32" xpath="/defaultno" width="300" />
		<Field id="status" desc="有效标示" edittype="select" translated="LIST:0,无效;1,有效"  xpath="/status" width="300" require="true"/>
		<Field id="opItemId" desc="关联功能组1"  edittype="select"  dropdowntype="dynamic" dataType="String" 
            translated="CQ:PageOptItem" viewField="name" init="true"  fieldmap="opItemId=id;opItemIdName=name"
            size="20" primary="false" type="String" xpath="/opItemId" width="300"/>
        <Field id="opItemIdName" desc="关联功能组"  xpath="/opItemId" method="com.huateng.ebank.business.common.CQMethod.getOpItemId"/>
		<Field id="lastUpdTs" desc="最后修改时间" edittype="text" datatype="string" size="17" xpath="/lastUpdTs" width="300"/>
		<Field id="lastUpdTlr" desc="最后修改操作员" edittype="text" datatype="string" size="20" xpath="/lastUpdTlr" width="300"/>
		
       </Fields>

	<Parameters></Parameters>

	<Operations>
		<Button id="ibtNew1" name="btNew1" desc="新增" operation="addrecord"  icon="icon-add"/>
		<Button id="ibtNew2" name="btNew2" desc="快速新增" operation="" url="" show="" group="" txn="" icon="icon-add"/>
		<Button id="ibtSave" name="btSave" desc="保存" operation="asysubmit" updateClass="com.huateng.ebank.business.pages.updater.FPPageItemUpdater" icon="icon-save"/>
		<Button id="ibtCopy" name="btCopy" desc="快速复制" operation="" updateClass="" url="#" txn="" icon="icon-restart-panel"/>
		<Button id="ibtView" name="btView" desc="详情" operation="" url="" show="" group=""  icon="icon-side-list"  />
		<Button id="ibtDel" name="btDel" desc="删除"  operation="asysubmit" updateClass="com.huateng.ebank.business.pages.updater.FPPageItemUpdater" show="" group="" txn="" icon="icon-cancel" />
		<Button id="ibtCheck" name="btCheck" desc="检测/预览" operation="" url="" show="" group="" txn="" icon="icon-default" />
		<Button id="ibtPro" name="btPro" desc="生成代码" operation="" url="" show="" group="" txn="" icon="icon-setting-upgrade" />
		<Button id="ibtEdit" name="btEdit" desc="修改" operation="" url="" show="" group="" txn="" icon="icon-edit" />
	</Operations>

	<Script></Script>

</CommQry>