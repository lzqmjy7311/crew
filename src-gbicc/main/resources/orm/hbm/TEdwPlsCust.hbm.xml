<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gbicc.personCommon.entity">

	<class name="TEdwPlsCust" table="T_EDW_PLS_CUST">

		<id name="custid" type="java.lang.String" column="CUSTID">
			<generator class="assigned" />
		</id>

		<property name="custname" type="string">
            <column name="CUSTNAME" length="60" />
        </property>
        <property name="paperkind" type="string">
            <column name="PAPERKIND" length="2" />
        </property>
        <property name="paperid" type="string">
            <column name="PAPERID" length="30" />
        </property>
        <property name="birtdate" type="date">
            <column name="BIRTDATE" length="10" />
        </property>
        <property name="sexsign" type="string">
            <column name="SEXSIGN" length="1" />
        </property>
        <property name="mobilecall" type="string">
            <column name="MOBILECALL" length="20" />
        </property>
        <property name="workcorp" type="string">
            <column name="WORKCORP" length="60" />
        </property>
        <property name="corpaddr" type="string">
            <column name="CORPADDR" length="100" />
        </property>
        <property name="waykind" type="string">
            <column name="WAYKIND" length="4" />
        </property>
        <property name="yearinco" type="big_decimal">
            <column name="YEARINCO" precision="12" />
        </property>
        <property name="balance" type="big_decimal">
            <column name="BALANCE" precision="12" />
        </property>
        <property name="amSalaryLast2M" type="big_decimal">
            <column name="AM_SALARY_LAST_2_M" precision="12" />
        </property>
        <property name="amSalaryLastM" type="big_decimal">
            <column name="AM_SALARY_LAST_M" precision="12" />
        </property>
        <property name="amPaymentLast2M" type="big_decimal">
            <column name="AM_PAYMENT_LAST_2_M" precision="12" />
        </property>
        <property name="amPaymentLastM" type="big_decimal">
            <column name="AM_PAYMENT_LAST_M" precision="12" />
        </property>
        <property name="status" type="string">
            <column name="STATUS" length="30" />
        </property>
        <property name="statusDate" type="date">
            <column name="STATUS_DATE" length="10" />
        </property>
        <property name="loanBalance" type="big_decimal">
            <column name="LOAN_BALANCE" precision="12" />
        </property>
        <property name="overCountMaxHis" type="java.lang.Integer">
            <column name="OVER_COUNT_MAX_HIS" />
        </property>
        <property name="unpaidCount" type="java.lang.Integer">
            <column name="UNPAID_COUNT" />
        </property>
        <property name="marrsign" type="string">
            <column name="MARRSIGN" length="1" />
        </property>
        <property name="educsign" type="string">
            <column name="EDUCSIGN" length="2" />
        </property>
        <property name="degreesign" type="string">
            <column name="DEGREESIGN" length="2" />
        </property>
        <property name="vocasign" type="string">
            <column name="VOCASIGN" length="3" />
        </property>
        <property name="natisign2" type="string">
            <column name="NATISIGN2" length="3" />
        </property>
        <property name="nativeplace" type="string">
            <column name="NATIVEPLACE" length="40" />
        </property>
        <property name="natisign1" type="string">
            <column name="NATISIGN1" length="4" />
        </property>
        <property name="natiflag" type="string">
            <column name="NATIFLAG" length="1" />
        </property>
        <property name="reginatu" type="string">
            <column name="REGINATU" length="1" />
        </property>
        <property name="contaddr" type="string">
            <column name="CONTADDR" length="42" />
        </property>
        <property name="workyearmonth" type="string">
            <column name="WORKYEARMONTH" length="6" />
        </property>
	</class>
</hibernate-mapping>
