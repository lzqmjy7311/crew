<#import "/templets/commonQuery/CommonQueryTagMacro.ftl" as CommonQueryMacro>
<#assign bean=JspTaglibs["/WEB-INF/struts-bean.tld"] />

<#assign warningId=RequestParameters["warningId"]?default("")>

<@CommonQueryMacro.page title="客户指标信息">
<table>
	<@CommonQueryMacro.CommonQuery id="C15" init="true" submitMode="current">
		<@CommonQueryMacro.DataTable id="C15Table" nowrap="true" height="305" readonly="true" fitColumns="false"
		fieldStr="customerNum,advanceAmount,advanceBalance,groupCustomerNum,lastMSalAmount,lastTwoMSalAmount,lastThreeMSalAmount,lastMPubAmount,lastTwoMPubAmount,lastThreeMPubAmount,coreCustomerNum,creditAmount,creditBalance,normalAdvanceBal,mentionAdvanceBal,substdAdvanceBalance,doutfulAdvanceBal,lossAdvanceBal,custManagerId,overdueAdvanceBal,outsideGuaranteeAmount,outstandingAdvanceCount,handlingOrgCd,corporative,isOverdue,fiveLevel,classificationDate,customerName,coreHandlingOrgCd,depositBal,groupName" width="100%" hasFrame="true"/>
	</@CommonQueryMacro.CommonQuery>
</table>
<script>
	//页面初始化
	function initCallGetter_post(){
		var warningId='${warningId}';
		C15_dataset.setParameter("warningId",warningId);
	}
</script>
</@CommonQueryMacro.page>